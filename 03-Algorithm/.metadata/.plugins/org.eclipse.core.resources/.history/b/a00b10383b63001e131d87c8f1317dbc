import java.io.*;
import java.util.*;

public class SWEA_1249_보급로_윤정인 {

	static int[][] map;
	static int[][] distance;

	public static void main(String[] args) throws IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringBuilder sb = new StringBuilder();
		int T = Integer.parseInt(br.readLine());

		for (int tc = 1; tc <= T; tc++) {
			int N = Integer.parseInt(br.readLine());
			map = new int[N][N];
			distance = new int[N][N];

			for (int i = 0; i < N; i++) {
				String str = br.readLine();
				for (int j = 0; j < N; j++)
					map[i][j] = str.charAt(j) - '0';
			}

			for (int i = 0; i < N; i++)
				for (int j = 0; j < N; j++)
					findRoute(i, j, N);

			sb.append("#" + tc).append(" " + distance[N - 1][N - 1]).append('\n');
		}

		System.out.print(sb);
	}

	private static void findRoute(int i, int j, int N) {
		
		int[][] delta = { { -1, 0 }, { 1, 0 }, { 0, -1 }, { 0, 1 } };

		for (int d = 0; d < 4; d++) {
			int x = i + delta[d][0];
			int y = j + delta[d][1];

			if (x < 0 || x >= N || y < 0 || y >= N)
				continue;

			if (distance[x][y] == 0) {
				if (x == 0 && y == 0) continue;
				distance[x][y] = distance[i][j] + map[x][y];
			}

			else {
				distance[x][y] = Math.min(distance[x][y], distance[i][j] + map[x][y]);
			}
		}
	}
}
